{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "script",
        "ecmaFeatures": {}
    },
    "rules": {
        "valid-jsdoc": 2,
        "switch-colon-spacing": 2,
        "spaced-comment": 2,
        "space-unary-ops": 2,
        "space-infix-ops": 2,
        "space-in-parens": 2,
        "space-before-function-paren": 2,
        "space-before-blocks": 2,
        "semi": 2,
        "semi-style": 2,
        "semi-spacing": 2,
        "require-jsdoc": 2,
        "quotes": 2,
        "quote-props": 2,
        "padding-line-between-statements": 2,
        "padded-blocks": 2,
        "operator-linebreak": 2,
        "object-property-newline": 2,
        "object-curly-spacing": 2,
        "object-curly-newline": 2,
        "no-whitespace-before-property": 2,
        "no-unreachable": 2,
        "no-unexpected-multiline": 2,
        "no-underscore-dangle": 2,
        "no-undefined": 2,
        "no-trailing-spaces": 2,
        "no-dupe-keys": 2,
        "no-empty-function": 2,
        "no-empty": 2,
        "no-extra-parens": 2,
        "no-extra-semi": 2,
        "no-invalid-this": 2,
        "no-irregular-whitespace": 2,
        "no-lonely-if": 2,
        "no-magic-numbers": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-multiple-empty-lines": 2,
        "no-const-assign": 2,
        "no-cond-assign": 2,
        "newline-before-return": 2,
        "linebreak-style": 2,
        "line-comment-position": 2,
        "keyword-spacing": 2,
        "key-spacing": 2,
        "indent": 2,
        "function-paren-newline": 2,
        "eqeqeq": 2,
        "eol-last": 2,
        "curly": 2,
        "comma-style": 2,
        "comma-spacing": 2,
        "comma-dangle": 2,
        "brace-style": 2,
        "block-spacing": 2,
        "array-bracket-spacing": 2,
        "array-bracket-newline": 2
    },
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "shared-node-browser": true,
        "worker": true,
        "amd": true,
        "mocha": true,
        "jasmine": true,
        "jest": true,
        "phantomjs": true,
        "jquery": true,
        "qunit": true,
        "prototypejs": true,
        "shelljs": true,
        "meteor": true,
        "mongo": true,
        "protractor": true,
        "applescript": true,
        "nashorn": true,
        "serviceworker": true,
        "atomtest": true,
        "embertest": true,
        "webextensions": true,
        "es6": true,
        "greasemonkey": true
    }
}